
public class TV {
	public int channel;
	public int volumeLevel;
	public boolean on;
	public TV() {
		channel = 1;
		volumeLevel = 3;
		on = false;
	}
	public void channelUp() {
		channel++;
	}
	
	public void channelDown() {
		channel--;
	}
	public void volumeUp() {
		volumeLevel++;
	}
	public void volumeDown() {
		volumeLevel--;
	}
}
