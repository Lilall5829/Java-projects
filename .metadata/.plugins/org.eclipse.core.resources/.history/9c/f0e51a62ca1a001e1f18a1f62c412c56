import java.util.*;
public class Car {
	private int year;
	private String make;
	private int mileage;
	private double price;
	private Date dateCreated;
	
	public Car() {
		dateCreated = new Date();
	}
	public Car(int year, String make, int mileage, int price) {
		this.year = year;
		this.make = make;
		this.mileage = mileage;
		this.price = price;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
}
